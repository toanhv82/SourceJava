<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="userSoa" class="com.ocb.ttt.service.dto.UserAPI">              
        <property name="user" value="soa"></property> 
        <property name="pw" value="2873565D88C15E8D8BA4C26FAA9B4FE6D4E16BD7440EA150822E7CF268AAA7AE"></property>  
        <property name="role" value="5"></property>  
    </bean> 
    <bean id="userSms" class="com.ocb.ttt.service.dto.UserAPI">              
        <property name="user" value="sms"></property> 
        <property name="pw" value="2873565D88C15E8D8BA4C26FAA9B4FE6D4E16BD7440EA150822E7CF268AAA7AE"></property>  
        <property name="role" value="6"></property>  
    </bean>  
    <bean id="managerApiUser"  class="com.ocb.ttt.sevice.security.ServiceUserLogin">        
        <property name="listUser">
            <list value-type="java.util.ArrayList">
                <ref bean="userSms"/>
                <ref bean="userSoa"/>
            </list>
        </property>
    </bean>     
</beans>